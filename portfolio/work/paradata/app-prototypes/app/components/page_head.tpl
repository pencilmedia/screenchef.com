<!-- BEGIN: Customer Accounts Page Header -->
<header class="page-head" data-id="@@accountsID">
	<h1>@@productTitle</h1>
	<nav class="pull-right page-actions">
		<button class="btn btn-link" id="addCustomer"><i class="fa fa-plus"></i>Add New Account</button>
		<button class="btn btn-link btn-icon open-contextual"><i class="fa fa-line-chart"></i></button>
		<button class="btn btn-link btn-icon"><i class="fa fa-cog"></i></button>
	</nav>
</header>
<!-- BEGIN: NMR Page Header -->
<header class="page-head score-box" data-id="@@nmrDetailsID">
	<div class="score good">76</div><div class="score okay">76</div><div class="score bad">76</div>
	<h1>@@pageTitle</h1>
	<p>John Smith &bull; 3 comments &bull; 1 day old</p>
</header>
<!-- BEGIN: Prototype Page Header -->
<header class="page-head" data-id="@@classProto">
	<h1>@@productProto</h1>
</header>